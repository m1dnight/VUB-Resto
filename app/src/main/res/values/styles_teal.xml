<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SplashTheme" parent="@android:style/Theme.Holo.NoActionBar">
        <item name="android:windowBackground">@color/l_white</item>
    </style>

    <style name="Theme.Teal" parent="@style/Theme.AppCompat">
        <item name="android:windowContentOverlay">@null</item>
        <item name="actionBarItemBackground">@drawable/selectable_background_teal</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Teal</item>
    </style>

    <style name="ActionBar.Solid.Teal" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_teal</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_teal</item>
    </style>


</resources>